 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  <servlet-name> sign_in </servlet-name>
  <servlet-class> adv_j_servlet03.StudentServlet </servlet-class>
  </servlet>
  <servlet-mapping> 
  <servlet-name> sign_in </servlet-name>
  <url-pattern>/signin</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
  <servlet-name> log_in </servlet-name>
  <servlet-class> adv_j_servlet03.LoginServlet </servlet-class>
  </servlet>
  <servlet-mapping> 
  <servlet-name> log_in </servlet-name>
  <url-pattern>/login</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name> update </servlet-name>
  <servlet-class> adv_j_servlet03.UpdateServlet </servlet-class>
  </servlet>
  <servlet-mapping> 
  <servlet-name> update </servlet-name>
  <url-pattern>/update</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
  <servlet-name> display </servlet-name>
  <servlet-class> adv_j_servlet03.DisplayServlet</servlet-class>
  </servlet>
  <servlet-mapping> 
  <servlet-name> display </servlet-name>
  <url-pattern>/display</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name> delete </servlet-name>
  <servlet-class> adv_j_servlet03.DeleteServlet</servlet-class>
  </servlet>
  <servlet-mapping> 
  <servlet-name> delete </servlet-name>
  <url-pattern>/delete</url-pattern>
  </servlet-mapping>
  
  
</web-app>
